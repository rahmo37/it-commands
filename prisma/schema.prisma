generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Command {
  id          BigInt   @id @default(autoincrement())
  title       String
  commandText String
  platform    Platform
  tags        String[] @default([])
  notes       String?
  copyCount   Int      @default(0)
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt
}

enum Platform {
  windows
  powershell
  linux
  mac
  network
}
